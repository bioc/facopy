year <- 2014
vers <- paste("R package")
citEntry(entry="Manual",
         title = "facopy",
         author = personList(as.person("David Mosen-Ansorena")),
         year = year,
         note = vers,
         textVersion =
         paste("David Mosen-Ansorena (",year, "). facopy. ", vers, ".", sep=""))
